{
  "extends": ["uploadcare"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "max-nested-callbacks": ["error", 3],
    "max-statements": ["error", 15],
    "no-console": "off",
    "no-negated-condition": "off"
  }
}
